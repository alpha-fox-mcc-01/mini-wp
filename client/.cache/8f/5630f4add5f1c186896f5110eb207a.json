p>\n      </div>\n</template>\n<script>\nname : 'loginPage'\nimport axios from 'axios'\nexport default {\n    data (){\n        return {\n            email:'',\n            password:''\n        }\n    },\n    props:{\n        page: String\n    },\n    methods:{\n        login(){\n             axios({\n                method: 'post',\n                url: 'http://localhost:3000/user/login',\n                data: {\n                    email: this.email,\n                    password: this.password\n                }\n            })\n                .then(({ data }) => {\n                    if(data.token){\n                        localStorage.setItem('access_token', data.token);\n                        console.log(data);\n                        this.email = ''\n                        this.password = ''\n                        this.changePage('home')\n                    }\n                    \n                })\n                .catch(err => {\n                    console.log(err)\n                })\n            \n        },\n         changePage(laman){\n             console.log('dari login page',laman);\n             this.$emit('change-Page', laman)\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"f9d4ea9f8a7b4d330388b83bbd72b26e","cacheData":{"env":{}}}