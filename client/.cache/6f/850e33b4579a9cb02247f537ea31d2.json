}\n            })\n                 .then(({ data }) => {\n                     console.log(data)\n                     this.articles = data.result\n                 })\n                 .catch(err => {\n                     console.log(err)\n                 })\n    },\n    userLogout() {\n            this.currentPage = 'landingpage'\n            localStorage.removeItem('access_token')\n    },\n     getDrafts() {\n            axios.get('http://localhost:3000/articles/drafts', {\n                headers: { \n                    access_token: localStorage.getItem('access_token')\n                }\n            })\n            .then(({ data }) => {\n                console.log(data)\n                this.drafts = data.result\n            })\n            .catch(err => {\n                console.log(err)\n            })\n    },\n  },\n  created: function(){\n    if(localStorage.getItem('access_token')){\n        this.currentPage = 'dashboard'\n    }\n    this.getArticles()\n    this.getDrafts()\n  }\n};\n</script>\n\n\n<style scoped>\n</style>"},"lineCount":null}},"error":null,"hash":"150c1e7508d2dd895a3c2689db41dc26","cacheData":{"env":{}}}