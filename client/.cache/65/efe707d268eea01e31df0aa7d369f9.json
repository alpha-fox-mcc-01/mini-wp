   console.log(data)\n                     this.articles = data.result\n                 })\n                 .catch(err => {\n                     console.log(err)\n                 })\n        },\n  },\n  created: function() {\n     this.getArticles()\n  },\n  updated : function() {\n    this.getArticles()\n  }\n}\n</script>\n\n\n<style>\n#headline {\n  background-image: url('https://cdn.pixabay.com/photo/2016/08/05/09/31/banner-1571858__340.jpg');\n  height: 15em;\n}\n\n#tagline {\n  padding-top: 5em;\n}\n\n\n</style>"},"lineCount":null}},"error":null,"hash":"52ea61869c5e70bdad070a2d28ff0fe8","cacheData":{"env":{}}}