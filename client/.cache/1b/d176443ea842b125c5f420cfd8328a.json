     this.password = ''\n                     console.log(data)\n                     localStorage.setItem('access_token', data.access_token) \n                     localStorage.setItem('username', data.name)\n                     this.changePage('dashboard') \n                 })\n                 .catch(err => {\n                     console.log(err)\n                 })  \n        },\n  }\n}\n</script>\n\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"c4b368751991aa5506383b39582a0e95","cacheData":{"env":{}}}